def rechercheMinMax(tab):
    if tab == []:
        return {'min': None, 'max': None}
    else:
        mini = tab[0]
        maxi = tab[1]
        for i in range(1, len(tab)):
            if tab[i] > maxi:
                maxi = tab[i]
            if tab[i] < mini:
                mini = tab[i]
        return {'min': mini, 'max': maxi}
tableau = [0, 1, 4, 2, -2, 9, 3, 1, 7, 1]

